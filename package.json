{
  "name": "lead-rating-system",
  "version": "1.0.0",
  "description": "AI-powered lead scraping and rating system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:api": "tsx src/api/server.ts",
    "worker:scraper": "tsx src/workers/scraper.worker.ts",
    "worker:processor": "tsx src/workers/processor.worker.ts",
    "worker:rating": "tsx src/workers/rating.worker.ts",
    "worker:automation": "tsx src/workers/automation.worker.ts",
    "worker:all": "concurrently \"npm:worker:*\"",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts"
  },
  "keywords": [
    "scraping",
    "ai",
    "lead-generation",
    "automation",
    "b2b"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "playwright": "^1.40.1",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "cheerio": "^1.0.0-rc.12",
    "axios": "^1.6.2",
    "axios-retry": "^4.0.0",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "langchain": "^0.1.0",
    "tiktoken": "^1.0.10",
    "bullmq": "^5.0.0",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "drizzle-orm": "^0.29.1",
    "minio": "^7.1.3",
    "nodemailer": "^6.9.7",
    "dotenv": "^16.3.1",
    "yaml": "^2.3.4",
    "winston": "^3.11.0",
    "prom-client": "^15.1.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.1",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21",
    "p-limit": "^5.0.0",
    "p-retry": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.14",
    "@types/lodash": "^4.14.202",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.10",
    "ts-jest": "^29.1.1",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
