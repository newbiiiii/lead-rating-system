{
  "name": "lead-rating-system",
  "version": "1.0.0",
  "description": "AI-powered lead scraping and rating system",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:api": "tsx src/api/server.ts",
    "worker:scraper": "tsx src/workers/scraper.worker.ts",
    "worker:processor": "tsx src/workers/processor.worker.ts",
    "worker:rating": "tsx src/workers/rating.worker.ts",
    "worker:crm": "tsx src/workers/crm.worker.ts",
    "worker:all": "concurrently \"npm:worker:*\"",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:rating": "jest test/rating",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:seed": "tsx src/db/seed.ts",
    "css:build": "node node_modules/@tailwindcss/cli/dist/index.mjs -i ./public/assets/css/tailwind.css -o ./public/assets/css/styles.css --minify",
    "css:watch": "node node_modules/@tailwindcss/cli/dist/index.mjs -i ./public/assets/css/tailwind.css -o ./public/assets/css/styles.css --watch",
    "docker:build": "docker build -t lead-rating-system .",
    "docker:dev": "docker run -p 3000:3000 -e NODE_ENV=development lead-rating-system",
    "docker:push": "echo 9tNykta8t39V | docker login 192.168.80.226:8080 -u admin --password-stdin && npm run docker:build && docker tag lead-rating-system:latest 192.168.80.226:8080/tes/lead-rating-system:latest && docker push 192.168.80.226:8080/tes/lead-rating-system:latest",
    "start:prod": "pm2-runtime start ecosystem.config.js"
  },
  "keywords": [
    "scraping",
    "ai",
    "lead-generation",
    "automation",
    "b2b"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.9.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.6.2",
    "axios-retry": "^4.0.0",
    "bullmq": "^5.0.0",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.1",
    "ioredis": "^5.9.0",
    "langchain": "^0.1.0",
    "lodash": "^4.17.21",
    "minio": "^7.1.3",
    "multer": "^2.0.2",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "p-limit": "^5.0.0",
    "p-retry": "^6.1.0",
    "pg": "^8.11.3",
    "playwright": "^1.40.1",
    "playwright-extra": "^4.3.6",
    "prom-client": "^15.1.0",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "tiktoken": "^1.0.10",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "@types/jest": "^29.5.10",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "concurrently": "^8.2.2",
    "drizzle-kit": "^0.20.6",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
